{
	"name": "webtrotion-astro-notion-cms-website-blog",
	"version": "1.3",
	"private": false,
	"cacheVersion": {
		"tmpJSONCache": "v1",
		"tmpHTMLCache": "vA",
		"tmpOGImagesCache": "v1",
		"publicNotion": "v1",
		"lastBuildTimestampStore": "vA"
	},
	"scripts": {
		"predev": "rm -rf buildcache && for folder in blocks-html-cache rss-cache; do if [ -d \"./tmp/$folder\" ]; then echo \"Deleting $folder folder\"; rm -rf \"./tmp/$folder\"; else echo \"No $folder folder to delete\"; fi; done",
		"dev": "astro dev",
		"prebuild-local": "rm -rf buildcache && for folder in blocks-html-cache rss-cache; do if [ -d \"./tmp/$folder\" ]; then echo \"Deleting $folder folder\"; rm -rf \"./tmp/$folder\"; else echo \"No $folder folder to delete\"; fi; done",
		"build-local": "astro build",
		"start": "astro dev",
		"build": "rm -rf buildcache && astro build",
		"postbuild": "pagefind --site dist",
		"preview": "astro preview",
		"format": "prettier -w --cache .",
		"check": "astro check",
		"build:cached": "npm run cache:fetch && astro build"
	},
	"dependencies": {
		"@astrojs/partytown": "^2.1.3",
		"@astrojs/rss": "^4.0.11",
		"@atproto/api": "^0.13.19",
		"@notionhq/client": "^2.2.15",
		"astro": "^5.3.0",
		"astro-robots-txt": "^1.0.0",
		"async-retry": "^1.3.3",
		"axios": "^1.7.9",
		"exif-be-gone": "^1.5.1",
		"fast-xml-parser": "^4.5.0",
		"katex": "^0.16.15",
		"mermaid": "^11.4.1",
		"metascraper": "^5.45.25",
		"metascraper-description": "^5.45.25",
		"metascraper-image": "^5.45.27",
		"metascraper-title": "^5.45.25",
		"metascraper-url": "^5.45.25",
		"png-to-ico": "^2.1.8",
		"sanitize-html": "^2.13.1",
		"satori-html": "^0.3.2",
		"sharp": "^0.33.5",
		"superjson": "^2.2.2"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@pagefind/default-ui": "^1.2.0",
		"@resvg/resvg-js": "^2.6.2",
		"@shikijs/transformers": "^1.24.2",
		"@tailwindcss/postcss": "^4.0.0",
		"@tailwindcss/vite": "^4.0.1",
		"@types/async-retry": "^1.4.9",
		"@types/eslint": "^9.6.1",
		"@types/katex": "^0.16.7",
		"@types/metascraper": "^5.14.3",
		"@types/metascraper-description": "^5.14.5",
		"@types/metascraper-image": "^5.14.5",
		"@types/sanitize-html": "^2.13.0",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"astro-eslint-parser": "^1.1.0",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.1",
		"mdast-util-to-string": "^4.0.0",
		"pagefind": "^1.2.0",
		"postcss": "^8.4.49",
		"postcss-html": "^1.7.0",
		"prettier": "^3.4.2",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"satori": "^0.12.0",
		"tailwindcss": "^4.0.2",
		"typescript": "^5.7.2"
	},
	"packageManager": "pnpm@9.15.0"
}
